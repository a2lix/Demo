{
	"type": "project",
	"license": "proprietary",
	"description": "Demo",
	"minimum-stability": "stable",
	"prefer-stable": true,
	"require": {
		"php": ">=8.2",
        "a2lix/auto-form-bundle": "dev-master",
        "a2lix/translation-form-bundle": "dev-master",
		"doctrine/doctrine-bundle": "^2.13.0",
		"doctrine/doctrine-migrations-bundle": "^3.3.1",
		"doctrine/orm": "^3.2.2",
		"gedmo/doctrine-extensions": ">=3.16.1",
		"symfony/asset": "^7.1.1",
		"symfony/asset-mapper": "^7.1.4",
		"symfony/console": "^7.1.4",
		"symfony/dotenv": "^7.1.3",
		"symfony/flex": "^2.4.6",
		"symfony/form": "^7.1.4",
		"symfony/framework-bundle": "^7.1.4",
		"symfony/monolog-bundle": "^3.10",
		"symfony/runtime": "^7.1.1",
		"symfony/security-bundle": "^7.1.4",
		"symfony/translation": "^7.1.3",
		"symfony/twig-bundle": "^7.1.1",
		"symfony/yaml": "^7.1.4",
		"twig/extra-bundle": "^3.13.0",
		"twig/twig": "^3.14.0"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.64",
		"kubawerlos/php-cs-fixer-custom-fixers": "^3.22",
		"rector/rector": "^1.2.5",
		"symfony/debug-bundle": "^7.1.1",
		"symfony/maker-bundle": "^1.61",
		"symfony/stopwatch": "^7.1.1",
		"symfony/web-profiler-bundle": "^7.1.4"
	},
	"replace": {
		"symfony/polyfill-php72": "*",
		"symfony/polyfill-php73": "*",
		"symfony/polyfill-php74": "*",
		"symfony/polyfill-php80": "*",
		"symfony/polyfill-php81": "*"
	},
	"config": {
		"allow-plugins": {
			"symfony/flex": true,
			"symfony/runtime": true
		},
		"optimize-autoloader": true,
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"auto-scripts": {
			"cache:clear": "symfony-cmd",
			"assets:install %PUBLIC_DIR%": "symfony-cmd",
			"importmap:install": "symfony-cmd"
		},
		"post-install-cmd": ["@auto-scripts"],
		"post-update-cmd": ["@auto-scripts"],
		"db-reset": [
			"bin/console d:d:d --if-exists --force",
			"bin/console d:d:c",
			"bin/console d:s:u --force"
		],
		"cs-fixer": ["php-cs-fixer fix --verbose"]
	},
	"conflict": {
		"symfony/symfony": "*"
	},
	"repositories": [
		{
			"type": "path",
			"url": "../AutoFormBundle",
            "options": {
                "symlink": true
            }
		},
		{
			"type": "path",
			"url": "../TranslationFormBundle",
            "options": {
                "symlink": true
            }
		}
	],
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "7.1.*",
			"docker": true
		}
	}
}
