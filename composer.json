{
    "type": "project",
    "license": "proprietary",
    "description": "Demo",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "a2lix/auto-form-bundle": "@dev",
        "a2lix/translation-form-bundle": "@dev",
        "doctrine/doctrine-bundle": "^3",
        "doctrine/doctrine-migrations-bundle": "^3",
        "doctrine/orm": "^3.6",
        "knplabs/doctrine-behaviors": ">=3",
        "stof/doctrine-extensions-bundle": "^1.15",
        "symfony/asset": "^8",
        "symfony/asset-mapper": "^8",
        "symfony/console": "^8",
        "symfony/dotenv": "^8",
        "symfony/expression-language": "^8",
        "symfony/flex": ">=2.10",
        "symfony/form": "^8",
        "symfony/framework-bundle": "^8",
        "symfony/intl": "^8",
        "symfony/monolog-bundle": ">=4.0",
        "symfony/runtime": "^8",
        "symfony/security-bundle": "^8",
        "symfony/translation": "^8",
        "symfony/twig-bundle": "^8",
        "symfony/validator": "^8",
        "symfony/yaml": "^8",
        "twig/extra-bundle": "^3.22",
        "twig/twig": "^3.22"
    },
    "repositories": [
        {
            "type": "path",
            "url": "/bundles/AutoFormBundle"
        },
        {
            "type": "path",
            "url": "/bundles/TranslationFormBundle"
        }
    ],
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.92",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.35",
        "rector/rector": "^2.3",
        "symfony/debug-bundle": "^8",
        "symfony/maker-bundle": ">=1.65.1",
        "symfony/stopwatch": "^8",
        "symfony/web-profiler-bundle": "^8"
    },
    "replace": {
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "db-reset": [
            "bin/console d:d:d --force --if-exists || true",
            "touch data.db || true",
            "bin/console d:s:u --force"
        ],
        "cs-fixer": [
            "php-cs-fixer fix --verbose"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "8.0.*",
            "docker": true
        }
    }
}
